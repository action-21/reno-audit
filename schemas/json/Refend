    $ref: https://schemas.pcl/1.0/Common#/$defs/Id
  description:
    $ref: https://schemas.pcl/1.0/Common#/$defs/Description
  surface:
    $ref: https://schemas.pcl/1.0/Common#/$defs/Surface
required:
  - id
  - description
  - surface
