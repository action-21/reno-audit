{
    "name": "renolab/audit",
    "type": "library",
    "description": "Service PHP de recherche et d'interprétation des Diagnostic de Performance Energétique (DPE)",
    "keywords": [
        "dpe",
        "audit",
        "3CL"
    ],
    "prefer-stable": true,
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Adrien Rosi",
            "email": "adrienrosi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/http-client": "^1.0",
        "psr/http-message": "^2.0",
        "symfony/http-client": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}